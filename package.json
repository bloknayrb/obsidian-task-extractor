{
  "name": "obsidian-task-extractor",
  "version": "2.3.1",
  "description": "An Obsidian plugin to extract tasks from email and meeting notes using LLMs",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "task",
    "llm",
    "ai"
  ],
  "author": "Bryan",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.1",
    "@types/codemirror": "^5.60.16",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
